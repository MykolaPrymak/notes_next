{
  "name": "notes_spa",
  "version": "0.0.1",
  "description": "Notes React Web Application",
  "source": "src/index.html",
  "repository": "git@github.com:MykolaPrymak/react_simple_form.git",
  "author": "Mykola Prymak <mykola.prymak@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "parcel",
    "prebuild": "run-s clean:build",
    "build": "parcel build --public-url './'",
    "build:analyzer": "parcel build --reporter @parcel/reporter-bundle-analyzer",
    "clean": "run-s clean:build clean:cache clean:analyzer",
    "clean:build": "rimraf ./dist",
    "clean:cache": "rimraf ./.parcel-cache",
    "clean:analyzer": "rimraf ./parcel-bundle-reports",
    "check": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint --ext ts,tsx src",
    "ci": "yarn build --no-source-maps && yarn test && yarn lint && yarn run check"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@reduxjs/toolkit": "^1.9.7",
    "markdown-to-jsx": "^7.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.10.1",
    "@parcel/transformer-inline-string": "^2.10.1",
    "@types/jest": "^29.5.6",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.10.1",
    "process": "^0.11.10",
    "typescript": "^5.2.2"
  }
}
